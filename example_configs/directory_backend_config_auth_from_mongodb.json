{
  "backend": {
    "module": "medallion.backends.directory_backend",
    "module_class": "DirectoryBackend",
    "path": "./example_configs/directory/",
    "discovery": {
      "title": "Some TAXII Server",
      "description": "This TAXII Server contains a listing of...",
      "contact": "string containing contact information",
      "host": "http://localhost:5000/"
    },
    "api-root": {
      "title": "",
      "description": "",
      "versions": [
        "taxii-2.0"
      ],
      "max-content-length": 9765625
    },
    "collection": {
      "id": "",
      "title": "",
      "description": "",
      "can_read": true,
      "can_write": false,
      "media_types": [
        "application/vnd.oasis.stix+json; version=2.0"
      ]
    }
  },
  "taxii": {
    "max_page_size": 100
  },
  "auth": {
    "module": "medallion.backends.auth_mongodb_backend",
    "module_class": "AuthMongodbBackend",
    "uri": "mongodb://root:example@localhost:27017/",
    "db_name": "auth"
  },
  "multi-auth": [
    "basic",
    "api_key"
  ],
  "flask": {}
}
